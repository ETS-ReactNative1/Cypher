{
  "expo": {
    "name": "cypher",
    "slug": "cypher",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icons/logo.png",
    "splash": {
      "image": "./app/assets/images/banner1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true

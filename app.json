      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icons/logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/icons/logo.png"
    },
    "description": "Cypher is an Advanced Crypto Wallet",
    "githubUrl": "https://github.com/acousticclown/cypher"
  }
}
